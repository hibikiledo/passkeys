{
	"compilerOptions": {
		"lib": ["ESNext", "DOM"],
		"module": "commonjs",
		"target": "es2019",
		"moduleResolution": "node",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"composite": true,
		"strict": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true
	}
}
